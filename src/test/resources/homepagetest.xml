<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Orange HRM Application">
	<parameter name="browsername" value="chrome"></parameter>
	<listeners>
		<listener class-name="com.UtilityLayer.ListenersSetUp"></listener>
		<listener
			class-name="com.UtilityLayer.AnnotationTransformerSetUp"></listener>
	</listeners>
	<test thread-count="5" name="Test">
		<classes>
			<class name="com.TestLayer.LoginPageTest" />
			<class name="com.TestLayer.HomePageTest" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
